    "query": "SELECT *,\n            (\n              6371 * acos(\n              cos(radians(?))\n              * cos(radians(lat))\n              * cos(radians(lon) - radians(?))\n              + sin(radians(?))\n              * sin(radians(lat))\n              )\n            ) AS distance\n            FROM\n            stations as s1\n            WHERE\n            e_status = 0\n            AND\n            station_cd = (\n              SELECT station_cd \n              FROM stations as s2\n              WHERE s1.station_g_cd = s2.station_g_cd\n              LIMIT 1\n            )\n            ORDER BY\n            distance\n            LIMIT ?"
  },
